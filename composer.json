{
  "name": "revolution/laravel-nostr",
  "description": "Laravel Nostr",
  "keywords": [
    "laravel",
    "nostr"
  ],
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "ext-gmp": "*",
    "illuminate/support": "^11.28",
    "guzzlehttp/guzzle": "^7.8",
    "swentel/nostr-php": "^1.4",
    "valtzu/guzzle-websocket-middleware": "^0.2.0",
    "uma/phpecc": "^0.1.3"
  },
  "require-dev": {
    "orchestra/testbench": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Revolution\\Nostr\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "kawax",
      "email": "kawaxbiz@gmail.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Revolution\\Nostr\\Providers\\NostrServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable"
}
